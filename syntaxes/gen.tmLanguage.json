{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Gen",
	"patterns": [
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numeric"
		}
	],
	"repository": {
		"keywords": {
			"patterns": [{
				"name": "keyword.control.gen",
				"match": "\\b(var|or|and|not|if|elseif|else|for|through|step|while)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.gen",
			"begin": "\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.gen",
					"match": "\\\\."
				}
			]
		},
		"numeric": {
			"patterns": [
				{
					"name": "constant.numeric.gen"
					"match": "\\b(0x[0-9a-fA-F]+|[0-9]+(\\.[0-9]+)?)\\b"
				}
			]
		}
	},
	"scopeName": "source.gen"
}
